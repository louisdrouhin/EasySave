{
  "config": {
    "logsPath": "./logs/",
    "stateFilePath": "./state.json",
    "cryptosoftPath": "./Cryptosoft.exe",
    "logFormat": "json",
    "maxConcurrentJobs": 3,
    "encryption": {
      "extensions": [".pdf", ".docx", ".xlsx", ".pptx", ".jpg", ".png", ".txt"]
    },
    "businessApplications": ["notepad"]
  },
  "easyLogServer": {
    "enabled": false,
    "mode": "local_only",
    "host": "localhost",
    "port": 5000
  },
  "jobs": [
    {
      "name": "test",
      "type": "Full",
      "sourceDir": "/path/source",
      "targetDir": "/path/destination"
    }
  ]
}
